package kr.or.ddit.whisp.service;

import java.sql.SQLException;
import java.util.List;

import kr.or.ddit.member.dao.MemberDao;
import kr.or.ddit.member.vo.MemberVO;
import kr.or.ddit.whisp.dao.WhispDao;
import kr.or.ddit.whisp.vo.WhispVO;

public class WhispService {
	// singleton 패턴 적용
	private WhispDao dao;
	
	public WhispService() {
		if(dao == null)
			dao = new WhispDao();
	}
	public List<WhispVO> retrieveWhispList(WhispVO whispVo) throws SQLException {
		List<WhispVO> list = dao.retrieveWhispList(whispVo);
		return list;
	}

	public void createMember(WhispVO whispVo) throws SQLException {
		dao.createWhisp(whispVo);
	}




	
}







package kr.or.ddit.whisp.vo;

public class WhispVO {
	private int ptNum;
	private String memId;
	private int whispDate;
	private String whispContent;
	
	public int getPtNum() {
		return ptNum;
	}
	public void setPtNum(int ptNum) {
		this.ptNum = ptNum;
	}
	public String getMemId() {
		return memId;
	}
	public void setMemId(String memId) {
		this.memId = memId;
	}
	public int getWhispDate() {
		return whispDate;
	}
	public void setWhispDate(int whispDate) {
		this.whispDate = whispDate;
	}
	public String getWhispContent() {
		return whispContent;
	}
	public void setWhispContent(String whispContent) {
		this.whispContent = whispContent;
	}
			
			
}

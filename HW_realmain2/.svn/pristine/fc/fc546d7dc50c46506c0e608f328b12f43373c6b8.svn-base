<sqlMap namespace="exerciseNotice">
	<insert id="insertexerciseNotice" parameterClass="kr.or.ddit.exerciseNotice.vo.ExerciseNoticeVO">
		INSERT INTO EXERCISENOTICE (
				TRAINER_ID
				,MEM_ID
				,EXERCISE
				,FOOD
				,MEM_DATE
		) VALUES (
			 #trainerId#
			,#memId# 
			,#exercise# 
			,#food#
			,#memDate#
		)
	</insert>
	
	
	
	
	
